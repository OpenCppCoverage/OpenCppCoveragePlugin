<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <!-- Basic settings -->
    <PropertyGroup>
        <!-- Prevents double inclusion of root configuration propers if project included with this condition -->
        <ConfigurationIncluded>yes</ConfigurationIncluded>

        <ShellVersion Condition="'$(VCProjectEngineVersion)'==''">14.0</ShellVersion>
        <VCProjectEngineVersion Condition="'$(VCProjectEngineVersion)'==''">14.0.0.0</VCProjectEngineVersion>
    </PropertyGroup>

    <PropertyGroup>
        <PlatformToolset>v140</PlatformToolset>
        <PlatformToolset Condition="'$(OverridePlatformToolSet)'!='false'">$(PlatformToolset)</PlatformToolset>
    </PropertyGroup>
</Project>
